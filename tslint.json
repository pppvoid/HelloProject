{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "semicolon": true,
        "class-name": false,
        "max-classes-per-file": false,
        "variable-name": false,
        "no-empty": false,
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "no-namespace": false,
        "prefer-for-of": false,
        "no-console": false
    },
    "rulesDirectory": []
}
